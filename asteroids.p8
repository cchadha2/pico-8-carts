pico-8 cartridge // http://www.pico-8.com
version 36
__lua__

pos = {}

function _init()
 pos.me_x = 64
 pos.me_y = 64
	pos.me_w = 10
end

function _draw()
	cls()
	spr(1,
					pos.me_x,
					pos.me_y) --,
					-- pos.me_w)
end

function _update()
 if btn(0) then 
		pos.me_x -= 1 
		pos.me_w += 10 
	end

	if btn(1) then 
		pos.me_x += 1 
		pos.me_w -= 10 
	end

 if btn(2) then pos.me_y-=1 end
 if btn(3) then pos.me_y+=1 end

	pos.me_w = pos.me_w % 360
end



